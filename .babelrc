{
  "presets": ["env", "react", "stage-3"],
  "plugins": [
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "src": "./src",
        "components": "./src/components",
        "i18n": "./i18n",
        "test": "./tests",
        "utils": "./src/utils"
      }
    }, "syntax-async-functions","transform-regenerator"]
  ]
}
